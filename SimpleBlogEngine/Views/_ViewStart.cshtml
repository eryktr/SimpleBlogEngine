@using System.Net
@using Microsoft.AspNetCore.Http;
@using Microsoft.AspNetCore.Mvc;

@{
    Layout = string.IsNullOrEmpty(Html.ViewContext.HttpContext.Session.GetString("Username")) 
        ? "_Layout"
        : Html.ViewContext.HttpContext.Session.GetString("isAdmin") == "True" ? "_LoggedAdminLayout" : "_LoggedLayout";
}


